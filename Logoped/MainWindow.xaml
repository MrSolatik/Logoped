<Window x:Class="Logoped.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Logoped"
        mc:Ignorable="d"
        Height="450" Width="700" MinHeight="250" WindowStartupLocation="CenterScreen" AllowsTransparency="True" WindowStyle="None">
    <Grid Background="#FF788285">
        <Border>
            <StackPanel>
                <!--Элементы для работы с БД-->

                <Frame Name="MainFrame" Height="390" VerticalAlignment="Bottom" NavigationUIVisibility="Hidden"
               ContentRendered="MainFrame_ContentRendered" Panel.ZIndex="-1"/>
            </StackPanel>
        </Border>
        <Button Name="BtnNext" Content="Переход далее" Style="{StaticResource 52}" Click="BtnNext_Click" HorizontalAlignment="Left" Margin="10,394,0,10" />
    </Grid>
</Window>
