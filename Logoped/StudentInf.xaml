<Page x:Class="Logoped.StudentInf"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Logoped"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="700"
      Title="StudentInf">

    <Grid>
        <!--Фрагмент загрузки word документа в форму-->
        <DocumentViewer Name="documentViewer1" HorizontalAlignment="Left" Panel.ZIndex="-1" Width="795" Margin="0,0,0,57"/>
        <TextBox Height="325" HorizontalAlignment="Center" Margin="0,40,0,0"
                Name="SelectedFileTextBox" VerticalAlignment="Top" Width="700" />
        <Button Content="Поиск" HorizontalAlignment="Left" Margin="0,398,0,0" Style="{StaticResource EditBtn}"
                Name="BrowseButton" VerticalAlignment="Top" Click="BrowseButton_Click" Height="52"/>
        <!--Конец-->
        <Button Name="SaveBtn" Content="Сохранить данные" Style="{StaticResource EditBtn}" Background="#FF37649B"
                Click="SaveBtn_Click" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="0 0 5 5"/>
    </Grid>
</Page>
